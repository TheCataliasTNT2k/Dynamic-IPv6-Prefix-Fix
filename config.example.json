{
  "listen_interface": "eth0",
  "upstream_mac": "",
  "ignore_new_prefixes": true,
  "prefixes_regex": "2001.*",
  "dhcpv6_process_filter": [
    "dhcpv6",
    "eth0"
  ],
  "send_interfaces": [
    {
      "name": "eth1",
      "send_delays": [
        1
      ],
      "prefixes": [
        {
          "regex": "2001.*",
          "length": 64
        }
      ]
    }
  ]
}
